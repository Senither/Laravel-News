{
    "name": "Laravel-News",
    "version": "0.1.0",
    "description": "A Discord Bot that pulls the latest news about Laravel",
    "main": "start.js",
    "maintainers": [
        {
            "name": "Alexis Tan",
            "email": "alexis@sen-dev.com"
        }
    ],
    "author": {
        "name": "Alexis Tan",
        "email": "alexis@sen-dev.com",
        "url": "https://senither.com"
    },
    "scripts": {
        "start": "node start.js",
        "test": "xo && mocha tests"
    },
    "engines": {
        "node": "4.x.x - 6.x.x",
        "npm": ">=3.x.x"
    },
    "xo": {
        "space": 4,
        "semicolon": true,
        "globals": [
            "app",
            "bot"
        ],
        "rules": {
            "camelcase": 0,
            "new-parens": 0,
            "guard-for-in": 0,
            "no-eval": 0,
            "no-unused-vars": 0,
            "no-prototype-builtins": 0,
            "unicorn/filename-case": 0,
            "import/no-dynamic-require": 0
        }
    },
    "license": "MIT",
    "dependencies": {
        "discordie": "^0.11.0",
        "feedme": "^1.0.0",
        "feedparser": "^2.1.0",
        "fs-extra": "^2.0.0",
        "html-md-2": "^3.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.18.1",
        "node-schedule": "^1.2.1",
        "request": "^2.81.0",
        "require-directory": "^2.1.1",
        "striptags": "^3.0.1",
        "winston": "^2.3.1",
        "winston-daily-rotate-file": "^1.4.6"
    },
    "devDependencies": {
        "minami": "^1.1.1",
        "mocha": "^3.2.0",
        "xo": "^0.18.0"
    },
    "snyk": true
}
